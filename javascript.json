{
  "Express 400 Response": {
    "prefix": "res400",
    "body": [
      "return res.status(400).send();"
    ],
    "description": "Send a 400 Bad Request response"
  },
  "Express 500 Response": {
  "prefix": "res500",
  "body": [
    "return res.status(500).json({",
    "  error: error.error,",
    "  message: error.message,",
    "  stack: error.stack",
    "});"
  ],
  "description": "Send a 500 Internal Server Error response"
},
